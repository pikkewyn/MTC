int main(){
    if( ( 5 + 0 ) != 5 ) return 1;
    if( ( 5 + 2 ) != 7 ) return 2;
    if( ( 1 + 2 + 3 ) != 6 ) return 3;
    if( ( 5 + -2 ) != 3 ) return 4;

    if( ( 5 - 0 ) != 5 ) return 5;
    if( ( 5 - 2 ) != 3 ) return 6;
    if( ( 5 - 2 - 1 ) != 2 ) return 7;
    if( ( 5 - -2 ) != 7 ) return 8;

    if( ( 4 * 1 ) != 4 ) return 9;
    if( ( 4 * 2 ) != 8 ) return 10;
    if( ( 2 * 3 * 4 ) != 24 ) return 11;

    if( ( 2 + 3 * 4 - 5 ) != 9 ) return 12;

    return 0;
}

